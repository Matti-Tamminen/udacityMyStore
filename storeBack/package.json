{
  "name": "storeback",
  "version": "1.0.0",
  "description": "MyStore backend",
  "main": "build/src/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --fix",
    "build": "tsc",
    "migrate": "db-migrate up",
    "jasmine": "jasmine",
    "test": "tsc && jasmine",
    "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./build/src/server.js\"",
    "start": "node .",
    "deploy": "eb init --profile default && eb use storeAPI-env --profile default && eb deploy --profile default"
  },
  "author": "MT",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Matti-Tamminen/udacityMyStore.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jasmine": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "node": "^16.9.1",
    "pg": "^8.7.1",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.9.1",
    "@types/node": "^16.9.1",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.3"
  }
}